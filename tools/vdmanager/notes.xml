<?xml version="1.0"?>
<!DOCTYPE vdmanager.dtd [
<!ELEMENT vdmanager (partitions|free|virtual_disks|virtual_block_devices)*>

<!ELEMENT partitions (partition)*>
<!ELEMENT partition (major, minor, blocks, star_sect, nr_sects, name)>
<!ELEMENT major (#PCDATA)>
<!ELEMENT minor (#PCDATA)>
<!ELEMENT blocks (#PCDATA)>
<!ELEMENT start_sect (#PCDATA)>
<!ELEMENT nr_sects (#PCDATA)>
<!ELEMENT name (#PCDATA)>

<!ELEMENT free (virtual_disk)?>
<!ELEMENT virtual_disks (virtual_disk)*>
<!ELEMENT virtual_disk (name, key, expiry, extents)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT key (#PCDATA)>
<!ELEMENT expiry (#PCDATA)>
<!ELEMENT extents (extent)*>
<!ELEMENT extent (disk, size, offset)>
<!ELEMENT disk (#PCDATA)>
<!ELEMENT size (#PCDATA)>
<!ELEMENT offset (#PCDATA)>

<!ELEMENT virtual_block_devices (virtual_block_device)*>
<!ELEMENT virtual_block_device (key, domain, vbdnum, mode)>
<!ELEMENT key (#PCDATA)>
<!ELEMENT domain (#PCDATA)>
<!ELEMENT vbdnum (#PCDATA)>
<!ELEMENT mode (#PCDATA)>
]>

<vdmanager>
</vdmanager>
